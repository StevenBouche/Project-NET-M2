<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:model="clr-namespace:LibraryProject.Business.Dto.Books;assembly=LibraryProject.Business.Dto">
    <DataTemplate x:Key="bookListDataTemplate" DataType="{x:Type model:BookDto}">
        <StackPanel VerticalAlignment="Top">
            <TextBlock Text="bouque" />
            <TextBox Text="{Binding Path=Name}" />
            <TextBox Text="{Binding Path=Author}" />
        </StackPanel>
    </DataTemplate>
  

</ResourceDictionary>